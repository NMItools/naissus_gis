Sub lab_cev_duz

	OnError Goto Greska
	
	If MapWinSTATUS(0) <> 1 
	Then 
		Exit Sub
	End If

	Set Map Redraw Off
	
	Set Map Layer LayerID("CEVOVOD") Label 
	Font ("Arial",256,8,255,16777215) 
	With Round(ObjectLen(obj, "m"),0.01) 
