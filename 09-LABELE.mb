'===================================================================================================================== 
' labele za CEVOVOD 

Sub lab_cev_ulice

	OnError Goto Greska
	
	If MapWinSTATUS(0) <> 1 
	Then 
		Exit Sub
	End If
	
	Set Map Redraw Off

	Set Map Layer LayerID("CEVOVOD") Label 
		Auto On
		Font ("Arial",256,8,255,16777215) 
		With ULICA 
			Position Center 
			Visibility Zoom (0,1000) 
			Units "m"  
			Auto Retry On 
			PartialSegments On
			Position Center
								
	Set Map Redraw On
	
	Exit Sub
	'	-------------------------
	Greska:
	Call ERRCreate(Err(), Error$(), "lab_cev_ulice")
	Call ERRShow()
	
End Sub
'
Sub lab_cev_mat_prec

	OnError Goto Greska
	
	If MapWinSTATUS(0) <> 1 
	Then 
		Exit Sub
	End If

	set map redraw off
	
	Set Map Layer LayerID("CEVOVOD") Label
		Font ("Arial",256,8,255,16777215)
		With MATERIJAL+DN_MM 
			Auto On 
			Overlap On 
			Duplicates On 
			PartialSegments On 
			Position Center
			
	set map redraw on

	Exit Sub
	'	-------------------------
	Greska:
	Call ERRCreate(Err(), Error$(), "lab_cev_mat_prec")
	Call ERRShow()	

End Sub
