					vidToolbar = True
				else 										 					
'					Print "BUTTONPAD sa imenom = " + imeToolbar + " je Nevidljiv"
					vidToolbar = False
				End If
				Exit For
			End If 
		End If   

	sledeci:
	Next

	' WIN_TOOLBAR  
