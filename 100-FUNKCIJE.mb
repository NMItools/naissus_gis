Function ProveriString(byval str as string) as Logical
    
    Dim k, OKchars as String
    Dim i, l, n, cr, zr as Integer

    OKchars = "0123456789,-"
    
    l = Len(str)
	
	cr = 0
	zr = 0
	
    For i = 1 to l
        k = Mid$(str, i, 1)
		
		If InStr(1, OKchars, k) = 0
        	Then
        		Note "Nedozvoljeni karakteri!"
        		ProveriString = false
        		exit function
        	else
        		Do case k

					Case "-"
						cr = cr+1
						kota_min = left$(s_Kote,i-1)
						kota_max = right$(s_Kote,i-1)
						Exit For
	
					Case ","
		        		zr = zr+1
				
				End Case
     	End If
	Next
    

    If (zr+cr = 0) or (zr > 0 and cr = 0)
    	then izotip = 1
			ProveriString = true
			Exit Function
		Else 
		    If (zr = 0 and cr = 1) 
		    then izotip = 2
				ProveriString = true
				Exit Function
			Else
				goto greska
			End If
    End If


greska:
